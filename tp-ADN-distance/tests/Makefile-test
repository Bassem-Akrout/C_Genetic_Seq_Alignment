		$(A_TESTER) $(DIRTEST)/ba52_recent_omicron.fasta 153 2000 $(DIRTEST)/wuhan_hu_1.fasta 116 1000  2> valgrind4perf2000_1000.output
	cat valgrind4perf2000_1000.output
	@echo "*******************************"

